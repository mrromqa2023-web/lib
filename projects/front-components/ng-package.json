{
	"$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
	"dest": "../../dist/front-components",
	"lib": {
		"entryFile": "src/public-api.ts"
	},
	"assets": [
		{
			"glob": "**/*.scss",
			"input": "src/lib/shared/styles",
			"output": "lib/shared/styles"
		},
		{
			"input": "src/lib/shared/assets",
			"glob": "**/*",
			"output": "lib/shared/assets"
		}
	]
}

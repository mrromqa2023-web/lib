{
	"name": "front-library",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"lib:build": "ng build front-components",
		"lib:watch": "ng build front-components --watch",
		"lib:publish": "npm run lib:build && cp .npmrc ./dist/front-components/ && cd dist/front-components && npm pack && npm publish",
		"test": "ng test",
		"tokens:update": "style-dictionary build",
		"storybook": "ng run front-components:storybook",
		"build-storybook": "ng run front-components:build-storybook",
		"lint": "ng lint",
		"lint:fix": "ng lint --fix",
		"format": "prettier --write \"**/*.{ts,html,css,scss,json,md}\"",
		"prepare": "husky install"
	},
	"private": true,
	"dependencies": {
		"@angular-eslint/eslint-plugin": "19.2.1",
		"@angular-eslint/template-parser": "19.2.1",
		"@angular/animations": "^19.1.0",
		"@angular/cdk": "^19.1.3",
		"@angular/common": "^19.1.0",
		"@angular/compiler": "^19.1.0",
		"@angular/core": "^19.1.0",
		"@angular/forms": "^19.1.0",
		"@angular/platform-browser": "^19.1.0",
		"@angular/platform-browser-dynamic": "^19.1.0",
		"@angular/router": "^19.1.0",
		"@maskito/angular": "^3.2.1",
		"@maskito/core": "^3.2.1",
		"@maskito/kit": "^3.2.1",
		"@typescript-eslint/eslint-plugin": "^8.26.0",
		"@typescript-eslint/parser": "^8.26.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "^0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^19.1.0",
		"@angular/build": "^19.1.0",
		"@angular/cli": "^19.1.0",
		"@angular/compiler-cli": "^19.1.0",
		"@chromatic-com/storybook": "^3.2.5",
		"@compodoc/compodoc": "^1.1.26",
		"@storybook/addon-a11y": "^8.6.4",
		"@storybook/addon-backgrounds": "^8.6.4",
		"@storybook/addon-designs": "^8.2.1",
		"@storybook/addon-docs": "^8.6.4",
		"@storybook/addon-essentials": "^8.6.4",
		"@storybook/addon-interactions": "^8.6.4",
		"@storybook/addon-onboarding": "^8.6.4",
		"@storybook/addon-storysource": "^8.6.11",
		"@storybook/addon-styling-webpack": "^1.0.1",
		"@storybook/angular": "^8.6.4",
		"@storybook/blocks": "^8.6.4",
		"@storybook/test": "^8.6.4",
		"@tinkoff/eslint-config": "^5.0.0",
		"@tinkoff/eslint-config-angular": "^5.0.0",
		"@tinkoff/prettier-config": "^5.0.0",
		"@types/jasmine": "~5.1.0",
		"angular-eslint": "19.2.1",
		"babel-loader": "^10.0.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.0.2",
		"eslint-import-resolver-typescript": "^3.8.3",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-sort-class-members": "^1.21.0",
		"eslint-plugin-storybook": "^0.11.4",
		"file-loader": "^6.2.0",
		"husky": "^9.1.7",
		"jasmine-core": "~5.5.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"lint-staged": "^15.4.3",
		"ng-packagr": "^19.1.0",
		"prettier": "^3.5.3",
		"prettier-eslint": "^16.3.0",
		"react": "^19.0.0",
		"sass": "^1.85.1",
		"sass-loader": "^16.0.5",
		"storybook": "^8.6.4",
		"storybook-dark-mode": "^4.0.2",
		"style-dictionary": "^4.3.0",
		"typescript-eslint": "^8.26.0"
	},
	"engines": {
		"node": "^18.19.1 || ^20.11.1 || >=22.0.0",
		"npm": "^6.11.0 || ^7.5.6 || >=8.0.0"
	},
	"author": "ssnab",
	"license": "ISC",
	"lint-staged": {
		"*.ts": [
			"prettier --write",
			"eslint --max-warnings 2 ."
		],
		"*.{html,scss}": [
			"prettier --write"
		],
		"*.{json,md}": [
			"prettier --write"
		]
	}
}
